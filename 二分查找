#include<iostream>
using namespace std;

const int MAXSIZE = 1000;
//顺序表的建立
typedef struct
{
	int elem[MAXSIZE];
	int length;
} SqList;

void CreatList(int n,SqList &L)
{
	for(int i = 0;i < n;i++)
	{
		cin>>L.elem[i];
	}
	L.length = n;
}
//二分查找
int BinarySearch(SqList L,int key)
{
	int low = 1;
	int high = L.length;
	while(low <= high)
	{
		int mid = (low + high)/2;
		if(key == L.elem[mid])
		{
			return mid;
		}
		else if(key < L.elem[mid])
		{
			high = mid - 1;
		}
		else
		{
			low = mid + 1;
		}
	}
	return 0;
} 
